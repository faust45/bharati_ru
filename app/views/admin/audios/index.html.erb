<% end %>

<div id="main_tabs">
  <div id="last_tracks" class="tab files">
    <ul class="tracks">
      <%= render :partial => 'audio_item', :collection => @audios %>
    </ul>
  </div>

  <div class="tab cont files">
    <div id="albums_dir" class="sub_cont">
      <ul> <%= render :partial => 'album_item', :collection => albums %> </ul>
    </div>

    <div id="tracks_list" class="sub_cont">
    </div>
  </div>

  <div class="tab">
  </div>
</div>

<div id="lect_edit">
  <%= render :partial => 'track_form' %>
</div>
