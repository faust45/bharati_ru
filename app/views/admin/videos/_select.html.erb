<div id="<%= div_id(category) %>">
  <div class="pseudo-link category"><%= category.name %></div>
  <%= text_field_tag :tag %>
  <%= link_to 'Add', admin_tags_path(category), :id => 'add', :class => 'pseudo-link add' %>
  <div id='tags' class="tags"><%= category.tag_list %></div>
</div>

<script>
jQuery(function ($) {
  new EditTags('#<%= div_id(category) %>', '<%= autocomplete_admin_tags_path(category)%>');
});
</script>
