<table>
  <% @centers.each do |center| %>
    <tr>
      <td><%= center.country %><td>
      <td><%= center.name %><td>
      <td><%= link_to(image_tag('edit.jpg', :width => '20', :height => '20'), edit_admin_center_path(center)) %><td>
    </tr>
  <% end %>
</table>

<%= will_paginate(@centers) %>
